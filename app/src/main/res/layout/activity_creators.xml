<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.Creators">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="172dp"
        android:text="Adrià Rocamora"
        app:layout_constraintStart_toEndOf="@+id/Dlpng"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageView
        android:id="@+id/Jrpng"
        android:layout_width="107dp"
        android:layout_height="155dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jr" />

    <ImageView
        android:id="@+id/Dlpng"
        android:layout_width="125dp"
        android:layout_height="459dp"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dl"
        tools:layout_editor_absoluteX="56dp" />

    <ImageView
        android:id="@+id/Arpng"
        android:layout_width="159dp"
        android:layout_height="404dp"
        android:layout_marginTop="104dp"
        app:layout_constraintTop_toBottomOf="@+id/Jrpng"
        app:srcCompat="@drawable/ar"
        tools:layout_editor_absoluteX="34dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginTop="64dp"
        android:text="Joaquín Roca"
        app:layout_constraintStart_toEndOf="@+id/Jrpng"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="172dp"
        android:text="David Lamas"
        app:layout_constraintStart_toEndOf="@+id/Dlpng"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/Jrbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:text="Minimo2"
        app:layout_constraintStart_toEndOf="@+id/Jrpng"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/Dlbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="24dp"
        android:text="Minimo2"
        app:layout_constraintStart_toEndOf="@+id/Dlpng"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/Arbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:text="Minimo2"
        app:layout_constraintStart_toEndOf="@+id/Dlpng"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />
</androidx.constraintlayout.widget.ConstraintLayout>